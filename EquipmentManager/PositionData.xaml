<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <ControlTemplate x:Key="PositionData">
        <Border Padding="10">
            <StackPanel DataContext="{Binding PosDataVM}">
                <TextBlock Text="kuku:"/>
                <TextBlock Text="{Binding Path=Kuku1}"/>
                <TextBlock Text="{Binding Path=Kuku2}"/>
                <TextBlock Text="{Binding Path=Kuku3}"/>
            </StackPanel>
        </Border>
    </ControlTemplate>

</ResourceDictionary>